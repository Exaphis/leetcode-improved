    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@medv/finder": "^3.0.0",
    "@plasmohq/messaging": "^0.1.0",
    "@plasmohq/storage": "^1.0.4",
    "framer-motion": "^9.0.2",
    "plasmo": "^0.65.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
