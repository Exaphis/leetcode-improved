{
  "name": "leetcode-improved",
  "displayName": "LeetCode Improved",
  "version": "0.0.3",
  "description": "A browser extension to improve your LeetCode quality of life.",
  "author": "Kevin Wu",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
